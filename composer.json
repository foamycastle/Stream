{
  "name": "foamycastle/stream",
  "description": "a library bases for building stream processors, including a stream manager",
  "minimum-stability": "dev",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Aaron Sollman",
      "email": "unclepong@gmail.com"
    }
  ],
  "keywords": [
    "streams","stream manager"
  ],

  "require": {
    "php": "^8.1",
    "foamycastle/helper": "dev-master"
  },
  "require-dev": {
    "foamycastle/probar": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Foamycastle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "test"
    }
  },
  "options": {
    "disable-tls": true
  }
}